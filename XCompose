# Esperanto
#-v-
<Multi_key> <C> <x> 	: "ƒà"   Ccircumflex # LATIN CAPITAL LETTER C WITH CIRCUMFLEX
<Multi_key> <c> <x> 	: "ƒâ"   ccircumflex # LATIN SMALL LETTER C WITH CIRCUMFLEX
<Multi_key> <G> <x> 	: "ƒú"   Gcircumflex # LATIN CAPITAL LETTER G WITH CIRCUMFLEX
<Multi_key> <g> <x> 	: "ƒù"   gcircumflex # LATIN SMALL LETTER G WITH CIRCUMFLEX
<Multi_key> <H> <x> 	: "ƒ§"   Hcircumflex # LATIN CAPITAL LETTER H WITH CIRCUMFLEX
<Multi_key> <h> <x> 	: "ƒ•"   hcircumflex # LATIN SMALL LETTER H WITH CIRCUMFLEX
<Multi_key> <J> <x> 	: "ƒ¥"   Hcircumflex # LATIN CAPITAL LETTER J WITH CIRCUMFLEX
<Multi_key> <j> <x> 	: "ƒµ"   hcircumflex # LATIN SMALL LETTER J WITH CIRCUMFLEX
<Multi_key> <S> <x> 	: "≈ú"   Hcircumflex # LATIN CAPITAL LETTER S WITH CIRCUMFLEX
<Multi_key> <s> <x> 	: "≈ù"   hcircumflex # LATIN SMALL LETTER S WITH CIRCUMFLEX
<Multi_key> <U> <x> 	: "≈¨"   Ubreve # LATIN CAPITAL LETTER U WITH BREVE
<Multi_key> <u> <x> 	: "≈≠"   ubreve # LATIN CAPITAL LETTER U WITH BREVE
#-^-

